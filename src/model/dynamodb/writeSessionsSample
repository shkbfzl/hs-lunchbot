var params = {
    TableName: "Sessions",
    Item: {
        "Id": "12345",
        "Users": [
            "JosiahWong", "Mr. Smith"
        ],
        "LastModified": "2016-09-12"
    }
};

docClient.put(params, function(err, data) {
    if (err)
        console.log(JSON.stringify(err, null, 2));
    else
        console.log(JSON.stringify(data, null, 2));
});